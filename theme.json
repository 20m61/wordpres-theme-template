			"button": {
				"border": {
					"radius": "30px"
				},
				"color": {
					"background": "var(--wp--preset--color--kawaii-pink)",
					"text": "var(--wp--preset--color--dark-gray)"
				},
				"typography": {
					"fontWeight": "700",
					"fontSize": "var(--wp--preset--font-size--medium)"
				},
				"spacing": {
					"padding": {
						"top": "0.75rem",
						"right": "1.5rem",
						"bottom": "0.75rem",
						"left": "1.5rem"
					}
				},
				"shadow": "var(--wp--preset--shadow--kawaii-soft)",
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--hot-pink)"
					},
					"shadow": "var(--wp--preset--shadow--kawaii-medium)"
				},
				":focus": {
					"shadow": "var(--wp--preset--shadow--kawaii-medium)",
					"border": {
						"color": "var(--wp--preset--color--hot-pink)",
						"width": "2px"
					}
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "700",
					"lineHeight": "1.2"
				},
				"color": {
					"text": "var(--wp--preset--color--dark-gray)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxx-large)",
					"fontWeight": "800"
				},
				"spacing": {
					"margin": {
						"bottom": "1.5rem"
					}
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				},
				"spacing": {
					"margin": {
						"top": "2rem",
						"bottom": "1rem"
					}
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				},
				"spacing": {
					"margin": {
						"top": "1.5rem",
						"bottom": "1rem"
					}
				}
			},
			"h4": {
