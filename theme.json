		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"css": ":root { --kawaii-border-radius: var(--wp--custom--kawaii--border-radius--medium); --kawaii-transition: var(--wp--custom--kawaii--transitions--normal); --kawaii-shadow: var(--wp--custom--kawaii--shadows--kawaii); }",
